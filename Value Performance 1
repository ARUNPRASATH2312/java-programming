import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner Sc=new Scanner(System.in);
        int a=Sc.nextInt();
        float b=Sc.nextFloat();
        System.out.println(a);
        System.out.printf("%.2f\n",b);    
        
        
    }
}
